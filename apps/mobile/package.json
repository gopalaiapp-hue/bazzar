{
    "name": "mobile",
    "version": "1.0.0",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "test": "jest",
        "prebuild": "expo prebuild --clean",
        "build:android:preview": "eas build --platform android --profile preview",
        "build:android:apk": "eas build --platform android --profile production-apk",
        "build:android:aab": "eas build --platform android --profile production",
        "build:local:apk": "cd android && ./gradlew assembleRelease",
        "build:local:aab": "cd android && ./gradlew bundleRelease"
    },
    "dependencies": {
        "expo": "~50.0.0",
        "expo-status-bar": "~1.11.1",
        "expo-localization": "~14.6.0",
        "expo-build-properties": "~0.11.0",
        "react": "18.2.0",
        "react-native": "0.73.2",
        "i18next": "^23.10.0",
        "react-i18next": "^14.0.5",
        "@react-native-async-storage/async-storage": "1.21.0",
        "tamagui": "^1.90.0",
        "@tamagui/core": "^1.90.0",
        "lucide-react-native": "^0.300.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@types/react": "~18.2.45",
        "@types/react-native": "^0.73.0",
        "@testing-library/react-native": "^12.4.3",
        "@types/jest": "^29.5.12",
        "jest": "^29.2.1",
        "jest-expo": "~50.0.0",
        "typescript": "^5.1.3",
        "eas-cli": "^7.0.0"
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
        ]
    },
    "private": true
}